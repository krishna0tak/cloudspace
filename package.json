{
  "name": "cloudspace",
  "private": true,
  "version": "0.1.0",
  "description": "Task Manager full-stack app (Express + React)",
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "concurrently -n server,client -c green,cyan \"npm run dev -w server\" \"npm run dev -w client\"",
    "dev:server": "npm run dev -w server",
    "dev:client": "npm run dev -w client"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
